{% embed 'PrestaShopBundle:Admin/Helpers:bootstrap_popup.html.twig' with {
    'id': 'bulk-print-modal',
    'title': 'Bulk Print label',
    'closable': true,
    'actions': [{
        'type': 'button',
        'label': 'Print',
        'class': 'btn btn-primary btn-lg js-submit-modal-form-btn bulk-print',
    }],
} %}
    {% block content %}
        <div class="modal-body">
            <form method="post" action="{{ path('admin_myparcel_orders_label_bulk_print') }}" data-bulk-inputs-id = "print_label_order_ids">
                <div id="print_label_order_ids" data-prototype="<input type=&quot;hidden&quot; id=&quot;print_label_order_ids___name__&quot; name=&quot;print_label[order_ids][__name__]&quot; required=&quot;required&quot; class=&quot;form-control&quot; />"></div>
                {% include '@PrestaShop/Admin/Sell/Order/Order/Blocks/print_position_block.html.twig' %}
            </form>
        </div>
    {% endblock %}
{% endembed %}

