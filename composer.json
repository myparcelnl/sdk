{
    "name": "myparcelnl/sdk",
    "version": "8.1.1",
    "description": "This package is designed to send and receive data from MyParcel by means of an API.",
    "keywords": [
        "MyParcel",
        "Flespakket",
        "PostNL"
    ],
    "homepage": "https://myparcelnl.github.io/sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Edie Lemoine",
            "email": "support@myparcel.nl"
        },
        {
            "name": "Joeri van Veen",
            "email": "support@myparcel.nl"
        },
        {
            "name": "Mark Ernst",
            "email": "support@myparcel.nl"
        },
        {
            "name": "Richard Perdaan",
            "email": "support@myparcel.nl"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit --coverage-html coverage"
        ]
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.16.0",
        "phpunit/phpunit": "^9.6.22",
        "psr/container": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "MyParcelNL\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyParcelNL\\Sdk\\Test\\": "test/"
        }
    }
}